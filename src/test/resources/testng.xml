<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Sample Test Suite" verbose="1">
    <test name="Method Test Cases">
        <classes>
            <class name="QuoterTest">
                <methods>
                    <exclude name="simpleInjectTest"/>
                    <exclude name="injectRandomIntTest"/>
                    <exclude name="profilingTest"/>
                    <exclude name="postProxyTest"/>
                    <exclude name="deprecatedClassTest"/>
                    <exclude name="propertyFileContextTest"/>
                    <include name="screenSaverTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>